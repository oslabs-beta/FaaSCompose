{
  "name": "faascompose",
  "version": "0.1.0",
  "description": "Graphical User Interface for Composing FaaS Workflows",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/FaaSCompose.git"
  },
  "keywords": [
    "faas",
    "compose",
    "workflow",
    "serverless"
  ],
  "author": "FaaSCompose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oslabs-beta/FaaSCompose/issues"
  },
  "homepage": "https://github.com/oslabs-beta/FaaSCompose#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@monaco-editor/react": "^3.5.7",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "bootstrap": "^4.5.2",
    "esprima": "^4.0.1",
    "fs": "0.0.1-security",
    "interweave": "^12.5.0",
    "jquery": "^3.5.1",
    "monaco-editor": "^0.20.0",
    "nanoid": "^3.1.12",
    "next": "9.5.2",
    "node-mocks-http": "^1.9.0",
    "openwhisk-composer": "^0.12.0",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-flow-renderer": "^5.1.2",
    "react-json-editor-ajrm": "^2.5.9",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@testing-library/react": "^10.4.9",
    "@types/enzyme": "^3.10.6",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.0.27",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.46",
    "@types/react-bootstrap": "^0.32.22",
    "@types/react-redux": "^7.1.9",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-jest": "^26.3.0",
    "typescript": "^3.9.7"
  }
}
